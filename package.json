{
  "name": "@marko/rollup",
  "description": "A Marko plugin for Rollup",
  "version": "2.1.1",
  "author": "Dylan Piercey <dpiercey@ebay.com>",
  "bugs": "https://github.com/marko-js/rollup/issues",
  "dependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/pluginutils": "^3.0.8",
    "concat-with-sourcemaps": "^1.1.0",
    "mz": "^2.7.0",
    "tslib": "^1.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.1.3",
    "@types/mz": "2.7.0",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "coveralls": "^3.0.9",
    "escape-string-regexp": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-file-snapshot": "^0.3.8",
    "lint-staged": "^10.0.7",
    "marko": "^4.18.39",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0",
    "rollup-plugin-postcss": "^2.1.1",
    "standard-version": "^8.0.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/marko-js/rollup",
  "keywords": [
    "loader",
    "marko",
    "plugin",
    "rollup",
    "transform"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "peerDependencies": {
    "marko": "^4",
    "rollup": "^1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marko-js/rollup"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "ci:report": "cat coverage/lcov.info | coveralls",
    "ci:test": "jest --ci --coverage",
    "format": "prettier \"**/*.{json,md,js,ts}\" --write",
    "lint": "tsc --noEmit && eslint -f codeframe './src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "release": "standard-version && git push --follow-tags origin master && npm publish",
    "test": "jest --watch",
    "test:inspect": "node --inspect $(which jest) --watch --runInBand"
  },
  "types": "dist/index.d.ts"
}
